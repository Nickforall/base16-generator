{
  "name": "base16-generator",
  "displayName": "Base16 Theme Generator",
  "description": "All Base16 themes available on the fly",
  "version": "1.0.0",
  "publisher": "golf1052",
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "themes": [
      {
        "label": "One Light",
        "uiTheme": "vs",
        "path": "./themes/base16-one-light.json"
      }
    ],
    "commands": [
      {
        "category": "Base16",
        "title": "Activate theme",
        "command": "extension.activateTheme"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "js-yaml": "^3.9.0",
    "mocha": "^2.3.3",
    "strip-json-comments": "^2.0.1",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}